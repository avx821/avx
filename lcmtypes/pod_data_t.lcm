package avionics;

struct pod_data_t
{
    int64_t  timestamp;
    double   gps[2];		//Decimal degrees lat, lon	
    double   pod_heading;   //magnetic heading(degrees (0,360]))	
    double   wind_data[2];	//wind magnitude (knots), true direction (degrees (0,360]))	
    boolean  pod_wave_off;   //1=Normal, 0 = Error
}